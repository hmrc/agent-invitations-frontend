# microservice specific routes

# Invitations ---------------------------------------------------------------------------------------------------------

# Agent Endpoints
GET         /agents                                             @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.agentsRoot
GET         /agents/identify-client                             @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.showIdentifyClientForm
POST        /agents/identify-client                             @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.submitIdentifyClient
GET         /agents/invitation-sent                             @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.invitationSent
GET         /agents/invitation-sent-continue                    @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.continueAfterInvitationSent
GET         /agents/select-service                              @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.selectService
POST        /agents/select-service                              @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.submitService
POST        /agents/fast-track                                  @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.agentFastTrack
GET         /agents/check-details                               @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.checkDetails
POST        /agents/check-details                               @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.submitDetails
GET         /agents/more-details                                @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.knownFact
POST        /agents/more-details                                @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.submitKnownFact

# Error Agent Endpoints
GET         /agents/not-enrolled                                              @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.notEnrolled
GET         /agents/not-matched                                               @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsInvitationController.notMatched

# Request tracking
GET         /track                                                            @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsRequestTrackingController.showTrackRequests
POST        /resend-link                                                     @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsRequestTrackingController.submitToResendLink
POST        /confirm-cancel                                                  @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsRequestTrackingController.submitToConfirmCancel
GET         /track/confirm-cancel                                            @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsRequestTrackingController.getConfirmCancel
POST        /track/confirm-cancel                                            @uk.gov.hmrc.agentinvitationsfrontend.controllers.AgentsRequestTrackingController.submitConfirmCancel

# Client Endpoints
GET         /:invitationId                                                    @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.start(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
POST        /:invitationId                                                    @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.submitStart(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
GET         /accept-tax-agent-invitation/confirm-decline/:invitationId        @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.getConfirmDecline(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
POST        /accept-tax-agent-invitation/confirm-decline/:invitationId        @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.submitConfirmDecline(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
GET         /accept-tax-agent-invitation/consent/:invitationId                @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.getConfirmTerms(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
POST        /accept-tax-agent-invitation/consent/:invitationId                @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.submitConfirmTerms(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
GET         /accept-tax-agent-invitation/accepted/:invitationId               @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.getCompletePage(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
GET         /reject-tax-agent-invitation/declined/:invitationId               @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.getInvitationDeclined(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)
GET         /decide-later/:invitationId                                       @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.getDecideLater(invitationId: uk.gov.hmrc.agentmtdidentifiers.model.InvitationId)

# Error Client Endpoints
GET         /not-signed-up/                                                   @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.notSignedUp
GET         /not-authorised/                                                  @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.notAuthorised
GET         /incorrect/                                                       @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.incorrectInvitation
GET         /not-found/                                                       @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.notFoundInvitation
GET         /already-responded/                                               @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.invitationAlreadyResponded
GET         /expired/                                                         @uk.gov.hmrc.agentinvitationsfrontend.controllers.ClientsInvitationController.invitationExpired

GET         /assets/*file                                                     controllers.Assets.at(path="/public", file)